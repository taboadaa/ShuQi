#ifndef APP_H_
#define APP_H_

#include <stdint.h>
#include <stdio.h>
#include "app_util.h"

#define RFID_ID_SIZE                     96/8                                       /**< Size of rfid id. */
#define RFID_ID_ARRAY_SIZE               20                                         /**< Size of rfid array. */

typedef struct {
	uint16_t size; /**< Number of array entries. */
	uint8_array_t * p_data; /**< Pointer to array entries. */
} rfid_ids_array_t;

static rfid_ids_array_t** rfid_ids;
static uint8_t nb_ids;



#endif /* APP_H_ */
