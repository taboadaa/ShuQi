#include "app.h"

void rfid_id_init(uint8_array_t** rfidId, ) {
	rfidId = calloc(sizeof(uint8_array_t)*RFID_ID_ARRAY_SIZE, 1);
}

void rfid_ids_add(uint8_array_t idTag) {
	// 96 bits idTag (96/8=12)
	if(idTag.size == 12) {
		uint32_t ii = 0;
		uint8_array_t* temp = rfid_ids[ii];

		while (temp != NULL || temp->) {
			temp
		}
		rfid_ids.size++;
	}
}

